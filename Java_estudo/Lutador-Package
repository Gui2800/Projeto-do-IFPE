package uef;
public class Uef {
    public static void main(String[] args) {
        Lutador l[] = new Lutador[6]; 
                
        l[0] = new Lutador("Gilberto Gil", "Brasil", 30, 1.80, 85.6, 10, 2, 5);
        l[1] = new Lutador("Alfredo Neves", "Inglaterra", 26, 1.74, 52.5, 10, 5, 2);
        l[2] = new Lutador("Fragnelson Jr.", "Alemanha", 21, 1.89, 87.4, 8, 5, 6);

        Luta UFC01 = new Luta();
        UFC01.marcarLuta(l[0], l[1]);
        UFC01.lutar();
    }
    
}
